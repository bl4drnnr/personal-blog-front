{
  "name": "personal-blog-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4202",
    "build": "ng build && node copy-proxy-server.js",
    "front:restart": "pm2 restart bahdashych-on-security-front",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "extract-i18n": "ng extract-i18n",
    "serve:ssr:bahdashych-on-security-front": "node dist/bahdashych-on-security-front/proxy-server.mjs",
    "fetch:slugs:dev": "NODE_ENV=development node fetch-slugs.mjs",
    "fetch:slugs:prod": "NODE_ENV=production node fetch-slugs.mjs",
    "format": "prettier --write \"{src,packages}/**/*.ts\" && eslint \"{src,packages}/**/*.ts\" --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "highlight.js": "^11.11.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.4",
    "@angular/cli": "^17.3.4",
    "@angular/compiler-cli": "^17.3.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^16.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "prettier": "^3.6.2",
    "typescript": "~5.4.2"
  }
}
